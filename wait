#include<bits/stdc++.h>
using namespace std;
int main()
{
    string s;
    getline(cin,s);
    string t;
    unordered_map<string,int>mp;
    istringstream inp(s);
    for(;inp>>t;)
        mp[t]++;
    bool flag=false;
    for(unordered_map<string,int>:: iterator it=mp.begin();it!=mp.end();it++)
    {
        if(it->second!=1){
            cout<<it->first<<" "<<it->second<<endl;
            flag=true;
        }
    }
    if(!flag) cout<<"-1"<<endl; 
    return 0;
}
